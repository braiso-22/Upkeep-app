<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.braiso_22.upkeep_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Upkeepapp">
        <activity
            android:name=".usecases.home.owner.OwnerHomeActivity"
            android:exported="false" />
        <activity
            android:name=".usecases.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".usecases.onboarding.OnBoardingActivity"
            android:exported="false" />
        <activity
            android:name=".usecases.splashscreen.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>